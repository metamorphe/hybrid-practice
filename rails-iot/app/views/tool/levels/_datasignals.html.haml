%event#timecut.signal-design
  %button.btn.btn-xs.btn-info.toggle
    =glyph "collapse-up"
  .vert
    %button#ts-cutter.vert-centered.design-title.btn-primary.trigger{:type => "trigger"}
      %span CUT
      %span.command [X]
  .track-full.droppable{:data =>{:accept=> 1, :draggable => "enabled", :semantic => "enabled", :timescale => 10000, :tracks => 1, :exportable => "disabled"}}
