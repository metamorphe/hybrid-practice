          <li><a id="load" name="load" href="#" class="process btn btn-lg btn-primary" data-toggle="modal" data-target="#open"><span class='glyphicon glyphicon-file'></a></li>
          <li><a id="pcb" name="pcb" href="#" class="process btn btn-lg btn-info"> Ci </a></li>
          <li><a id="spacers" name="spacers" href="#" class="process btn btn-lg btn-danger"> Sp </a></li>
          <li><a id="mold" name="mold" href="#" class="process btn btn-lg btn-success"> M </a></li>
          <li><a id="mask" name="mask" href="#" class="process btn btn-lg btn-primary"> Ma </a></li>
          <li><a id="code" name="code" href="#" class="process btn btn-lg btn-default"> Co </a></li>
      </ul>
      <p id="current-file" class="lead"> /artwork/sun_moon.svg </p>
    </div>
      
      <%= render :partial => "modals"%>
      <%= render :partial => "zoom_bar"%>

