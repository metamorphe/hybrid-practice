/ UPLOAD
#upload.modal.fade{"aria-labelledby" => "myModalLabel", :role => "dialog", :tabindex => "-1"}
  .modal-dialog{:role => "document"}
    .modal-content
      .modal-header
        %button.close{"aria-label" => "Close", "data-dismiss" => "modal", :type => "button"}
          %span{"aria-hidden" => "true"} Ã—
        %h4#myModalLabel.modal-title Upload File
      .modal-body
        %p.lead Choose a File to Upload ...
        %input.svg-upload{:type => "file"}/
      .modal-footer
        %button#upload-button.btn.btn-danger{"data-dismiss" => "modal"}
          Upload SVG
        %button.btn.btn-default{"data-dismiss" => "modal", :type => "button"} Close
        / <button type="button" class="btn btn-primary">Save changes</button>
